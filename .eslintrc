{
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [2, 2],
    "linebreak-style": [2, "unix"
    ],
    "semi": [2, "always"],
    "quotes": ["error", "single"],
    "react/prop-types": 0,
    "object-curly-spacing": [2, "always", {
      "objectsInObjects": true,
      "arraysInObjects": false
    }]
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
